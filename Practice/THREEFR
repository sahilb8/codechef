#include <stdio.h>

int main(void) {
	// your code goes here
	int n;

	scanf("%d",&n);
	for(int i=0;i<n;i++)
    {
        int a,b,c;
      scanf("%d %d %d",&a,&b,&c);
    if(a+b==c)
	printf("yes\n");
	else if(a-b==c)
	printf("yes\n");
	else if(a-b==-1*c)
	printf("yes\n");
	else
	printf("no\n");

    }

	return 0;
}




